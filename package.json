    "sandpack-vue3": "^3.1.12",
    "vfonts": "^0.0.3"
  },
  "volta": {
    "node": "18.17.1"
  },
  "peerDependencies": {
    "markdown-it": "^14.1.0",
    "react": "^19.2.0",
